exports[`Integration: module samples Module: depends_on between modules 1`] = `
"module "networking" {
  source   = "./modules/networking"
  vpc_cidr = "10.0.0.0/16"
}

module "database" {
  source     = "./modules/rds"
  vpc_id     = module.networking.vpc_id
  depends_on = [module.networking]
}

module "application" {
  source            = "./modules/ecs"
  vpc_id            = module.networking.vpc_id
  database_endpoint = module.database.endpoint
  depends_on        = [module.networking, module.database]
}
"
`;

exports[`Integration: module samples Module: provider pass-through 1`] = `
"provider "aws" {
  alias  = "east"
  region = "us-east-1"
}

provider "aws" {
  alias  = "west"
  region = "us-west-2"
}

module "vpc_east" {
  source = "terraform-aws-modules/vpc/aws"
  cidr   = "10.0.0.0/16"

  providers = {
    aws = aws.east
  }
}

module "vpc_west" {
  source = "terraform-aws-modules/vpc/aws"
  cidr   = "10.1.0.0/16"

  providers = {
    aws = aws.west
  }
}
"
`;

