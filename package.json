{
  "name": "react-hcl",
  "version": "0.2.2",
  "description": "JSX/TSX to Terraform (.tf) transpiler with a custom JSX runtime",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js"
    }
  },
  "bin": {
    "react-hcl": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bun run build:js && bun run build:types",
    "build:js": "bun run build:js:cli && bun run build:js:index && bun run build:js:jsx-runtime",
    "build:js:cli": "bun build src/cli/index.ts --outfile=dist/cli.js --format=esm --target=node --external=esbuild --external=commander --external=hcl2-parser --bundle --banner='#!/usr/bin/env node'",
    "build:js:index": "bun build src/index.ts --outfile=dist/index.js --format=esm --target=node --bundle",
    "build:js:jsx-runtime": "bun build src/jsx-runtime.ts --outfile=dist/jsx-runtime.js --format=esm --target=node --bundle",
    "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "lint": "bunx biome check .",
    "lint:fix": "bunx biome check --fix .",
    "typecheck:examples": "tsc -p examples/tsconfig.json --noEmit",
    "test": "bun test",
    "test:smoke:packed-install": "bash scripts/smoke-test-packed-install.sh",
    "tf:validate:examples": "bash scripts/terraform-validate-examples.sh",
    "prepack": "bun run build"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.15",
    "@types/bun": "^1.3.9"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "commander": "^14.0.1",
    "esbuild": "^0.27.3",
    "hcl2-parser": "^1.0.3"
  }
}
