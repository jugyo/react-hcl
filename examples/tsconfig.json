{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "jsxImportSource": "react-hcl",
    "baseUrl": "..",
    "paths": {
      "react-hcl": ["./src/index.ts"],
      "react-hcl/jsx-runtime": ["./src/jsx-runtime.ts"]
    }
  },
  "include": ["./**/*.tsx"]
}
